{
    "extends": "../../tsconfig.json",
    "include": ["./src"],
    "compilerOptions": {
      "outDir": "./dist"
    "allowSyntheticDefaultImports": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "declaration": true,
    "target": "esnext",
    "module": "es2020",
    "strict": true,
    "resolveJsonModule": true
    }
  }
