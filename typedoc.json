{
  "mode": "file",
  "out": "out",
  "exclude": "test",
  "theme": "minimal",
  "excludePrivate": true,
  "excludeNotExported": true,
  "listInvalidSymbolLinks": true,
  "target": "ES5",
  "moduleResolution": "node",
  "preserveConstEnums": "true",
  "stripInternal": "true",
  "module": "commonjs",
  "readme": "none",
  "exclude": "*.spec.ts",
  "external-modulemap": ".*packages\/(@namespace\/[^\/]+)\/.*",
  "excludeExternals": false
}
