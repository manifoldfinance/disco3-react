    "test:ci": "jest -u -c jest.config.js --passWithNoTests --coverageDirectory .github/coverage",
    "format": "prettier --config .prettierrc.js --write \"**/*.{ts,tsx,md}\"",
    "deduplicate": "npx yarn-deduplicate -s fewer yarn.lock",
